<Window x:Class="UBB_SE_2024_Team_42.GUI.SettingsPop_Up"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UBB_SE_2024_Team_42.GUI"
        mc:Ignorable="d"
        Title="SettingsPop_Up" Height="350" Width="800">
    <Grid Margin="0,0,0,-62" Background="#FF625ED7">
        <TextBlock Text="Settings" Margin="41,10,511,211" FontSize="50" FontWeight="Bold"></TextBlock>
        <Button x:Name="Edit_Button" Margin="41,109,104,168" Background="Black" Foreground="#FFF7F7F7" Click="Edit_Button_Click">
            <DockPanel>
                <Image/>
                <TextBlock Text="EDIT" FontSize="40" HorizontalAlignment="Left" VerticalAlignment="Center" FontWeight="Bold"/>
            </DockPanel>
        </Button>
        <Button x:Name="Delete_Button" Margin="41,279,104,10" Background="#FF6C06AA" Foreground="White" Click="Delete_Button_Click">
            <DockPanel>
                <Image/>
                <TextBlock FontSize="40" Text="DELETE" FontWeight="Bold"/>
            </DockPanel>
        </Button>
    </Grid>
</Window>
